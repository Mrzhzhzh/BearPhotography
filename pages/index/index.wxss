/**index.wxss**/
@import "../../wxParse/wxParse.wxss";
page{
	width:100%;
	min-height:100%;
	background:#f5f5f5;
}

/***************menu****************/
.menu{
	width:100%;
	height:210rpx;
}
.menu_item{
	width:20%;
	height:110rpx;
	display:inline-block;
	position:relative;
	margin-top:50rpx;
}
.menu_boxs{
	width:110rpx;
	height:110rpx;
	position:relative;
	border-radius:50%;
	overflow:hidden;
	text-align:center;
	margin:0 auto;
}
.menu_item image{
	width:110rpx;
	height:110rpx;
}
.menu_box{
	width:110rpx;
	height:110rpx;
	position: absolute;
	left:0;
	top:0;
	text-align: center;
	line-height:110rpx;
}
.store_img{
	width:100%;
	height:330rpx;
	margin-bottom:24rpx;
}
.store_img image{
	width:100%;
	height:100%;
}
.ip_tab_comtainer{
	width: 100%;
    white-space: nowrap;
    height:210rpx;
    background:#fff;
}
.wxParse-p image{
	margin:0 auto;
	width:100%;
	height:100%;
}
.mask_cont{
	width:100%;
	height:100%;
	position:relative;
}
.mask_cont button{
	width:100%;
	border-radius:0;
	position:absolute;
	left:0;
	bottom:120rpx;
}